<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="org.doit.ik.mapper.EmpMapper">
	<select id="IdCheck" resultType="_int">
		SELECT COUNT(*)
		FROM emp
		WHERE empno = #{empno}
	</select>
</mapper>